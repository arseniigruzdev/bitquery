{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_SUPABASE_URL": "${supabase_url}",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": "${supabase_anon_key}",
    "BITQUERY_API_KEY": "${bitquery_api_key}"
  }
}
